html{color:#000;font-family:inter,sans-serif}body{max-width:920px;margin:0 auto;padding:1rem}.inline-svg{display:inline-block;height:1.15rem;width:1.15rem;top:.15rem;position:relative}.index{display:flex;justify-content:space-between}.index aside{margin-right:1rem;margin-top:1rem;max-width:350px}.index aside .person{display:flex}.index aside h1{margin:0 auto;margin-top:3rem;margin-left:1rem}.index aside p{margin:0 auto;color:#838383;margin-bottom:1rem;margin-left:1rem}.index aside img{border-radius:50%;margin-bottom:1rem}.index aside ul{padding:0;list-style-type:none}.index aside ul svg{width:16px;margin-left:-2px;fill:#57606a}.index aside ul a{text-decoration:none}.index aside ul a:hover{text-decoration:underline}.index aside li{margin-top:.2rem;margin-bottom:.2rem}@media(max-width:640px){.index aside{display:none}}.index section .article{margin:0 auto;margin-bottom:1rem}footer{display:flex;justify-content:space-between;margin-top:2rem}footer a{text-decoration:none}footer a:hover{text-decoration:underline}@media(max-width:640px){footer{display:none}}.back-home{margin-top:2rem}.back-home a{text-decoration:none}.back-home a:hover{text-decoration:underline}